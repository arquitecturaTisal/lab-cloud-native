services:
  serviciovalidaad_openshift:
    build:
      context: C:\TFS\tfs.tisal.cl\Desarrollo Interno\Laboratorios\Cloud Native\ServicioValidaAD_OpenShift
      dockerfile: ServicioValidaAD_OpenShift/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44395'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      LOGIN_BASEURL: http://200.6.103.165/
      LOGIN_URLRELATIVA: UsuarioActiveDirectory/api/v1/
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: serviciovalidaadopenshift:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/ServicioValidaAD_OpenShift.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 56587
      target: 80
    - published: 44395
      target: 443
    volumes:
    - C:\TFS\tfs.tisal.cl\Desarrollo Interno\Laboratorios\Cloud Native\ServicioValidaAD_OpenShift\ServicioValidaAD_OpenShift:/app:rw
    - C:\Users\lpetruzzella\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\lpetruzzella\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\lpetruzzella\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\lpetruzzella\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'